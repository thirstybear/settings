alias showFiles='defaults write com.apple.finder AppleShowAllFiles YES; killall Finder /System/Library/CoreServices/Finder.app'
alias hideFiles='defaults write com.apple.finder AppleShowAllFiles NO; killall Finder /System/Library/CoreServices/Finder.app'

alias docker_killall='docker kill $(docker ps -q)'
alias docker_rmall='docker rm $(docker ps -aq)'
alias docker_nukeall='docker_killall; docker_rmall'
alias docker-gc='docker run --rm -v /var/run/docker.sock:/var/run/docker.sock spotify/docker-gc'

alias safari="open -a safari"
alias gw=./gradlew

alias projects='cd ~/dev/projects'

alias use_java8="export JAVA_HOME=`/usr/libexec/java_home -v 1.8`;java -version"
alias use_java11="export JAVA_HOME=`/usr/libexec/java_home -v 11`;java -version"

alias kc=kubectl
